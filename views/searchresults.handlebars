<div class="uk-section 404">
  <h2>Here are your delicious results</h2>

  <table class="uk-table uk-table-striped">
    <thead>
      <tr>
        <th>Beer</th>
        <th>Brewery</th>
        <th>Style</th>
      </tr>
    </thead>


    <tbody>

      {{#each beerName}}
      <tr>
        <td><a class="beerClick" href="/beer/{{this.id}}">{{this.beer_name}}</a></td>
        <td><a class="breweryClick" href="/brewery/{{this.brewery_name}}">{{this.brewery_name}}</a></td>
        <td>{{this.beer_style}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>

  <ul class="uk-pagination uk-flex-center">
    <li><a href="#">1</a></li>
    <li><a href="#">2</a></li>
    <li><a href="#">3</a></li>
    <li><a href="#">4</a></li>
    <li><a href="#">5</a></li>
  </ul>

</div>
