<h4>Here are your results for "{{userInput}}"</h4>

<table class="uk-table uk-table-striped uk-table-hover">
  <thead>
    <tr>
      <th>Beer</th>
      <th>Brewery</th>
      <th>Style</th>
      <th>ID</th>
    </tr>
  </thead>


  <tbody>

    {{#each beerName}}
    <tr>
      <td><a class="beerClick" href="/beer/{{this.id}}">{{this.beer_name}}</a></td>
      <td><a class="breweryClick"  href="/brewery/{{this.id}}">{{this.brewery_name}}</a></td>
      <td>{{this.beer_style}}</td>
      <td>{{this.id}}</td>
    </tr>
    {{/each}}

